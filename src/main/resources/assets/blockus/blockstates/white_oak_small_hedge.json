{
    "multipart": [
        {   "when": { "up": "true" },
            "apply": { "model": "blockus:block/white_oak_small_hedge_end" }
        },
        {   "when": { "north": "true" },
            "apply": { "model": "blockus:block/white_oak_small_hedge_side", "uvlock": true }
        },
        {   "when": { "east": "true" },
            "apply": { "model": "blockus:block/white_oak_small_hedge_side", "y": 90, "uvlock": true }
        },
        {   "when": { "south": "true" },
            "apply": { "model": "blockus:block/white_oak_small_hedge_side", "y": 180, "uvlock": true }
        },
        {   "when": { "west": "true" },
            "apply": { "model": "blockus:block/white_oak_small_hedge_side", "y": 270, "uvlock": true }
        }
    ]
}