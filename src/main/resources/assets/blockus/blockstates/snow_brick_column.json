{
  "multipart": [
    {
      "apply": {
        "model": "blockus:block/snow_brick_column_center"
      }
    },
    {
      "when": {
        "down": true
      },
      "apply": {
        "model": "blockus:block/snow_brick_column_end"
      }
    },
    {
      "when": {
        "up": true
      },
      "apply": {
        "model": "blockus:block/snow_brick_column_end",
        "x": 180,
        "uvlock": true
      }
    }
  ]
}
